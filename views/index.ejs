<% include ./partials/header %>

<div class="ui container">
    
<% blogs.forEach(function(blog) { %>
    <h2><%= blog.title %></h2>
    <img src="<%= blog.image %>">
    <span style="display:block"><%= blog.created %></span>
    <p><%= blog.body %></p>
<% }) %>

</div>

<% include ./partials/footer %>